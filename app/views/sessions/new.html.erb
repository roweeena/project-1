<h1>Log In</h1>


<%# NON CRUD %>
<%= form_tag login_path do %>
  <%= label_tag :email, "email:" %>
  <%= email_field_tag :email,nil, :class => 'form-control' %>

  <%= label_tag :password, "password:" %>
  <%= password_field_tag :password,nil, :class => 'form-control' %>

  <%= submit_tag "Sign In", :class => 'submit' %>
<% end %>
